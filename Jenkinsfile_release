pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    // Build stage
    stage('Build Core Image') {
        def tagNo = sh(script: "git describe", returnStdout: true).toString().trim()
      steps {
        sh "echo ${env.tagNo}"
      }
    }
  }

}
